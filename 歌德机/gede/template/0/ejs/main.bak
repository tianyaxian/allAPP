<link href="/template/0/css/all.css" rel="stylesheet" type="text/css"/>
<script src="/template/0/js/all.js" type="text/javascript"></script>

<!--main start-->
<div id="container">
    <div class="zcenter">
        <div class="zlogo">
            <img id="gede_logo" src="/images/null.png" onclick="try{fw.showlocker();}catch(err){console.log(err.message);}"/>
        </div>
        <div class="zcontent">
            <div class="zbox">
                <div class="clear"></div>
            </div>
        </div>
    </div>
</div>
<div id="content_div" style="display: none">
    <div class="morph-content">
        <div style="height:100%; width:100%">
            <div class="spinner"></div>
            <iframe id="appFrame" style="height:100%; width:100%; display:none; overflow: auto" frameborder="0" marginheight="0" marginwidth="0"></iframe>
            <div class="icon icon-close" style="position: fixed; bottom:0px; left: 0px; width:80px; height:92px; z-index: 99999">
                <img style="border-radius:0px" src="/template/0/images/home.png"/>
            </div>
        </div>
    </div>
</div>
<!--main end-->
<!-- clock -->
<div class="clock" style="position: fixed; right:100px; top: 10px;">
    <embed wmode="Transparent" type="application/x-shockwave-flash" menu="true" loop="true" play="true" quality="high" allowfullscreen="true" showmovieinfo="0" src="/template/0/images/clock.swf" />
</div>
<div class="marquee" style="position: fixed; left:0px; top: 1030px; width:100%; ">
    <marquee id="marquee" scrollamount="10" width="100%" style="width: 100%; font-size: 40px; color: white;"></marquee>
</div>